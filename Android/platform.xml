	<!-- If you're including Android library projects with resources -->
	<packagedResources>
	    <!--
	    Copy the jars from the Android library project's 'libs' folder to this folder (not into the 'libs' subfolder).
	    Merge the 'src' folder of the Android library project into the 'src' subfolder here.
	    Copy the 'res' folder of the Android library project into this folder (not the 'libs' subfolder) and rename it.
	    If the project has native binaries in 'libs/armeabi-v7a', copy them here to 'libs/armeabi-v7a'.
	    Copy the packageName from the Android library project's AndroidManifest.xml to the packagedResource below.
		Specify the new folderName of the 'res' folder as you named it.
	     -->
		<packagedResource>
			<packageName>com.google.android.gms</packageName>
			<folderName>google-play-services-res</folderName>
		</packagedResource>
